?dexp
fillexp = function (rateparam=1/5, quantile = .25, min=0, max=20, step=.1){
x <- seq(min, max, step)
y <- dexp(x, rate = rateparam)
plot(x,y, type='l', xlab = 'Survial time in years', ylab = 'Density')
xmax <- qexp(quantile, rate=rateparam)
xtemp <- c(seq(0,xmax, step))
xcoords <- c(xtemp, rev(xtemp))
ycoords <- c(dexp(xtemp, rate=rateparam), rep(0, length(xtemp)))
polygon(xcoords, ycoords, col='grey')
}
